nohup sh -c 'for i in GCF_009828535.2_HMol_V2_genomic GCF_011100555.1_mCalJa1.2.pat.X_genomic GCF_015252025.1_Vero_WHO_p1.0_genomic GCF_016699345.2_BCM_Sbol_2.1_genomic GCF_020740605.2_mLemCat1.pri_genomic GCF_024542745.1_ASM2454274v1_genomic GCF_027406575.1_mNycCou1.pri_genomic ;do  /zhlab/software/blast/ncbi-blast-2.14.0+/bin/blastn -word_size 11 -reward 2 -penalty -3 -gapopen 5 -gapextend 2 -query /bio/hxl/outLTR12/outfile_LTR12C_.txt -db /bio/hxl/all_GCFA/${i} -out /bio/hxl/outblast/LTR12C_/${i}.txt -evalue 1E-5 -num_threads 50 -outfmt "6 delim=  qaccver saccver pident qlen qcovs qcovhsp qcovus length mismatch gapopen qstart qend sstart send evalue bitscore" -max_target_seqs 10000 -max_hsps 50000 ;done;' 2>> /bio/hxl/outblast/LTR12C_/LTR12C.log &
